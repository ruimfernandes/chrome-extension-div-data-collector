{
  "manifest_version": 2,
  "name": "Div data collector",
  "description": "Collects data from divs with specific class",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "icons": { "128": "logo_128.png" },
  "permissions": ["tabs", "<all_urls>"],
  "background": {
    "scripts": ["bg.js"]
  }
}
